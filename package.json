{
  "name": "feathers-typescript-starter",
  "description": "A feathersjs starter app. Built using feathersjs, typescript and swagger for hackathons",
  "version": "0.0.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers",
    "typescript",
    "swagger"
  ],
  "license": "MIT",
  "author": {
    "name": "Thilak Rao",
    "email": "me@thilak.in",
    "url": "thilak.in"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": "^8.9.4",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "yarn run eslint && yarn run mocha",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "start": "run serve",
    "serve": "./node_modules/nodemon/bin/nodemon.js --watch \"src/**/*.js\" --ignore \"test/**/*.test.js\" --exec \"./node_modules/ts-node/dist/bin.js\" src/index.js",
    "mocha": "mocha test/ --recursive --exit"
  },
  "dependencies": {
    "@feathersjs/configuration": "1.0.2",
    "@feathersjs/errors": "3.3.0",
    "@feathersjs/express": "1.2.0",
    "@feathersjs/feathers": "3.1.3",
    "@feathersjs/socketio": "3.2.0",
    "compression": "1.7.2",
    "cors": "2.8.4",
    "helmet": "3.11.0",
    "serve-favicon": "2.4.5",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "eslint": "4.18.0",
    "mocha": "5.0.1",
    "nodemon": "1.15.1",
    "request": "2.83.0",
    "request-promise": "4.2.2",
    "ts-node": "4.1.0",
    "typescript": "2.7.2"
  }
}
